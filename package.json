{
	"name": "joke",
	"displayName": "joke",
	"description": "show a random joke",
	"version": "0.0.2",
	"icon": "src/resources/laugh.png",
	"repository": {
		"type": "git",
    	"url": "https://github.com/dreamfly2012/vscode_extension_joke"
	},
	"publisher":"menghuiguli",
	"keywords": [
		"笑话",
		"笑话大全",
		"幽默"
	],
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:view.joke",
		"onCommand:extension.joke"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "jokeMenu",
					"title": "Joke",
					"icon": "src/resources/joke.svg"
				}
			]
		},
		"views": {
			"jokeMenu": [
				{
					"id": "view.joke",
					"name": "Joke"
				}
			]
		},
		"commands": [
			{
				"command": "joke.prev",
				"title": "上一页",
				"icon": {
					"dark": "src/resources/dark/prev.svg",
					"light": "src/resources/light/prev.svg"
				}
			},
			{
				"command": "joke.next",
				"title": "下一页",
				"icon": {
					"dark": "src/resources/dark/next.svg",
					"light": "src/resources/light/next.svg"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.19.2"
	}
}
